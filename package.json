{
  "private": true,
  "scripts": {
    "ng": "ng",
    "build": "npm run build:lib",
    "build:lib": "npm run build:lib:common && npm run build:lib:core && npm run build:lib:worker && npm run build:lib:worker-client",
    "build:lib:core": "ng build core && cpy LICENSE.md dist/core/",
    "build:lib:common": "ng build common && cpy LICENSE.md dist/common/",
    "build:lib:worker": "ng build worker && cpy LICENSE.md dist/worker/",
    "build:lib:worker-client": "ng build worker-client && cpy LICENSE.md dist/worker-client/",
    "test": "npm run test:common && npm run test:core && npm run test:worker && npm run test:worker-client",
    "test:common": "ng test common --watch=false --progress=false --browsers=ChromeHeadlessCI --code-coverage",
    "test:core": "ng test core --watch=false --progress=false --browsers=ChromeHeadlessCI --code-coverage",
    "test:worker": "ng test worker --watch=false --progress=false --browsers=ChromeHeadlessCI --code-coverage",
    "test:worker-client": "ng test worker-client --watch=false --progress=false --browsers=ChromeHeadlessCI --code-coverage",
    "lint": "ng lint",
    "release": "semantic-release"
  },
